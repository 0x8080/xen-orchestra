    "xo-lib": "^0.8.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-closure-elimination": "^1.1.14",
    "clarify": "^2.0.0",
    "event-to-promise": "^0.7.0",
    "lodash": "^4.16.1",
    "mocha": "^3.0.2",
    "must": "^0.13.2",
    "source-map-support": "^0.4.2",
    "standard": "^8.1.0",
    "trace": "^2.3.3"
  },
  "scripts": {
    "build": "babel --source-maps --out-dir=dist/ src/",
    "dev": "babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
