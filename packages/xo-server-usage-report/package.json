{
  "name": "xo-server-usage-report",
  "version": "0.2.1",
  "license": "AGPL-3.0",
  "description": "",
  "keywords": [
    "orchestra",
    "plugin",
    "report",
    "usage",
    "xen",
    "xen",
    "xen-orchestra"
  ],
  "homepage": "https://github.com/vatesfr/xo-server-usage-report",
  "bugs": "https://github.com/vatesfr/xo-server-usage-report/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatesfr/xo-server-usage-report.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@vates.fr"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/",
    "images/",
    "report.html.tpl"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "cron": "^1.2.1",
    "handlebars": "^4.0.6",
    "html-minifier": "^3.4.0",
    "lodash": "^4.17.4",
    "promise-toolbox": "^0.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-stage-3": "^6.22.0",
    "cross-env": "^3.1.4",
    "dependency-check": "^2.8.0",
    "rimraf": "^2.6.1",
    "standard": "^9.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "clean": "rimraf dist/",
    "depcheck": "dependency-check ./package.json",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prebuild": "npm run clean",
    "predev": "npm run clean",
    "prepublish": "npm run build"
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-3"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  }
}
